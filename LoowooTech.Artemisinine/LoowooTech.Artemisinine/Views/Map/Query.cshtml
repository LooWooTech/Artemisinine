@{
    Layout = "~/Views/Map/_Layout2.cshtml";
    List<LoowooTech.Artemisinine.Models.DiseaseBase> list = ViewBag.List;
    int serial = 1;
}
<table class="table table-bordered">
    <tr>
        <th>序号</th>
        <th>时间</th>
        <th>数据</th>
    </tr>
    @foreach (var item in list)
    {
        <tr>
            <td>@(serial++)</td>
            <td>@(item.Time.ToShortDateString().ToString())</td>
            <td>@(Math.Round(item.Data,4))</td>
        </tr>
    }
</table>




