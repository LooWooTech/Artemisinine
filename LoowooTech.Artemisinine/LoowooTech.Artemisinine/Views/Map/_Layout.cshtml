@{
    Layout = null;
    string arcgisServer = AppSettings.Current["SERVER"];
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="initial-scale=1,maximum-scale=1,user-scalable=no" />
    <title>@(ViewBag.Title)</title>
    <link rel="stylesheet"  href="~/Content/bootstrap.css" />
    <link rel="stylesheet"  href="~/Content/bootswatch.min.css" />
    <link rel="stylesheet"  href="~/Content/main.min.css" />
    <script src="~/Scripts/jquery-1.9.1.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script>
        var arcgisServer = "@(arcgisServer)";
    </script>
    <link rel="stylesheet" type="text/css" href="http://@(arcgisServer)/arcgis_js_api/library/3.14/3.14/dijit/themes/tundra/tundra.css" />
    <link rel="stylesheet" type="text/css" href="http://@(arcgisServer)/arcgis_js_api/library/3.14/3.14/esri/css/esri.css" />
    <link rel="stylesheet" type="text/css" href="http://@(arcgisServer)/arcgis_js_api/library/3.14/3.14/dijit/themes/claro/claro.css" />
    <script src="http://@(arcgisServer)/arcgis_js_api/library/3.14/3.14/init.js"></script>
</head>
<body class="claro">
    @RenderBody()
</body>
</html>
