@{
    ViewBag.Title = "上传疾病数据";
}
<div class="container">
    <div class="panel panel-default">
        <div class="panel-heading">@(ViewBag.Title)</div>
        <div class="panel-body">
            <form class="form-horizontal" method="post" action="/Admin/Upload" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="Thing" class="control-label col-lg-3">疾病事件</label>
                    <div class="col-lg-3">
                        <input type="text" name="Thing" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="control-label col-lg-3">年份(选填)</label>
                    <div class="col-lg-3">
                        <input type="text" name="Year" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="control-label col-lg-3">请选择疾病文件：</label>
                    <div class="col-lg-3">
                        <input type="file" class="form-control" name="file" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-lg-3 col-lg-offset-3">
                        <button type="submit" class="btn btn-success">上传</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>